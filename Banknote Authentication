   "metadata": {},
   "outputs": [],
   "source": [
    "error_test2 = []\n",
    "for i in arange(1, 902, 10):\n",
    "    knn = KNeighborsClassifier(n_neighbors=i, \n",
    "                               weights='distance', \n",
    "                               metric='manhattan')\n",
    "    knn.fit(X_train, y_train)\n",
    "    pred_i = knn.predict(X_test)\n",
    "    error_test2.append(np.mean(pred_i != y_test))\n",
    "best_error2=min(error_test2)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "<font size=4>**Chebyshev**</font>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "error_test3 = []\n",
    "for i in arange(1, 902, 10):\n",
    "    knn = KNeighborsClassifier(n_neighbors=i, \n",
    "                               weights='distance', \n",
    "                               metric='chebyshev')\n",
    "    knn.fit(X_train, y_train)\n",
    "    pred_i = knn.predict(X_test)\n",
    "    error_test3.append(np.mean(pred_i != y_test))\n",
    "best_error3=min(error_test3)"
   ]
  },
