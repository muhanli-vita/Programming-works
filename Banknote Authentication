     ]
    }
   ],
   "source": [
    "data = pd.read_csv('data_banknote_authentication.txt', \n",
    "                   names = ['variance','skewness','curtosis','entropy','class'])\n",
    "df = data[data['class'] == 0]\n",
    "test0 = df.head(200)\n",
    "train0 = df[df.index >= 200]\n",
    "df = data[data['class'] == 1]\n",
    "test1 = df.head(200)\n",
    "rows = df.iloc[:,0].size\n",
    "train1 = df.tail(rows-200)\n",
    "from numpy import *\n",
    "test = np.vstack((test0, test1))\n",
    "train = np.vstack((train0, train1))\n",
    "print('The test set:\\n', test)\n",
    "print('\\nThe training set:\\n', train)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## (c) Classification using KNN\n",
    "### i. k-nearest neighbors with Euclidean metric (use sklearn package)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "knn = KNeighborsClassifier()\n",
    "X = train[:, [0,1,2,3]]\n",
    "y = train[:, 4]\n",
    "knn.fit(X, y)\n",
    "test_set = test[:, [0,1,2,3]]\n",
    "predict = knn.predict(test_set)"
   ]
  },
