     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "k = arange(1, 902, 3)\n",
    "x = 1/k\n",
    "fig = plt.figure()\n",
    "ax = fig.add_subplot(1, 1, 1)\n",
    "ax.semilogx(x, error_train, marker='.')\n",
    "ax.semilogx(x, error_test, marker='.')\n",
    "plt.xlabel('1/k')\n",
    "plt.ylabel('error')\n",
    "plt.legend(['train error','test error'])\n",
    "plt.show()"
   ]
  },
